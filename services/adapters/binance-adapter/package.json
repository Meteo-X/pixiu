{
  "name": "@pixiu/binance-adapter",
  "version": "1.0.0",
  "description": "Binance交易所适配器SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects='Unit Tests'",
    "test:integration": "jest --selectProjects='Integration Tests'",
    "test:performance": "jest --selectProjects='Performance Tests' --verbose",
    "test:all": "jest --selectProjects='Unit Tests' 'Integration Tests' 'Performance Tests'",
    "test:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "test:debug": "jest --runInBand --no-cache --verbose",
    "test:memory": "node --expose-gc --max-old-space-size=4096 ./node_modules/.bin/jest --logHeapUsage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "clean": "rm -rf dist coverage .jest-cache",
    "prebuild": "npm run clean",
    "pretest": "npm run build"
  },
  "keywords": ["binance", "trading", "adapter", "sdk", "cryptocurrency"],
  "author": "Pixiu Team",
  "license": "MIT",
  "dependencies": {
    "@pixiu/shared-core": "1.0.0",
    "@pixiu/adapter-base": "1.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@pixiu/test-utils": "^1.0.0",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.11",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "jest-watch-typeahead": "^2.2.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}